   "source": [
    "# lnblist.py\n",
    "\n",
    "# Given input lists\n",
    "L1 = [11, 21, 24, 12, 18]\n",
    "L2 = [14, 44, 25, 37, 13]\n",
    "\n",
    "# Create L3 by taking items at odd indices from L1 and even indices from L2\n",
    "L3 = L1[::2] + L2[1::2]\n",
    "\n",
    "# Display the result\n",
    "print(\"L3 =\", L3)\n"
   ]
